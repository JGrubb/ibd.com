<div id="archive" class="row">
  <h2 class="twelve column">All blog posts</h2>
    <% @posts_by_year.each do |year| %>
      <h5 id="<%= year[0] %>"><%= year[0] %></h5>
      <% year[1].each do |post| %>
        <div class="post">
          <h5><%= link_to post.title, post_date_path_helper(post) %><small> - <%= post.published_at.strftime('%D') %></small></h5>
        </div>
      <% end %>
    <% end %>
</div>
