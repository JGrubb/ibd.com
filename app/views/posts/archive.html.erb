<div id="archive" class="row">
  <h2 class="large-12 column">All blog posts</h2>
  <% @posts.each do |post| %>
    <div class="post large-3 medium-6 column">
      <h5><%= link_to post.title, post_date_path_helper(post) %></h5>
      <time><%= post.created_at.strftime('%b %d, %Y') %></time>
    </div>
  <% end %>
</div>
